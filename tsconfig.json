{
  "compilerOptions": {
    "target": "ES2019",
    "module": "commonjs",
    "jsx": "react-jsx",
    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "@/*": ["*"],
      "@/Contexts/*": ["Contexts/*"],
      "@/mocks/*": ["__mocks__/*"]
    },
    "types": ["jest", "node"]
  },
  "include": ["src/**/*", "src/global.d.ts"],
  "exclude": ["node_modules", "dist"]
}
